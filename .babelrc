{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      }
    }],
    "react"
  ],
  "plugins": [
    ["system-import-transformer", { "loose": true }],
    ["transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ],
    "transform-es2015-modules-umd"
  ]
}
